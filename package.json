{
  "name": "myshoppinglist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android-dev-debug": "ENVFILE=.env.dev react-native run-android --variant DevelopmentDebug",
    "android-dev-release": "ENVFILE=.env react-native run-android --variant DevelopmentRelease",
    "android-prod-debug": "ENVFILE=.env react-native run-android --variant ProductionDebug",
    "android-prod-release": "ENVFILE=.env react-native run-android --variant ProductionRelease",
    "setup": "yarn install_dependencies && yarn pod-install && yarn clean-project",
    "setup_m1": "yarn install_dependencies && yarn pod-install-m1 && yarn clean-project",
    "install_dependencies": "rm -rf node_modules && yarn install --force",
    "clean-project": "cd android && ./gradlew clean && cd .. && yarn cache clean",
    "pod-install": "cd ios && pod deintegrate && pod install && cd ..",
    "pod-install-m1": "cd ios && arch -x86_64 pod deintegrate && bundle exec arch -x86_64 pod install && cd ..",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format-staged-code": "pretty-quick --staged --pattern \"**/*.ts{,x}\"",
    "ts-check": "tsc",
    "static-code-validator": "yarn ts-check && yarn lint",
    "pre-commit": "yarn format-staged-code && yarn static-code-validator",
    "build-android": "cd android && ./gradlew assembleProductionRelease && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.1",
    "@react-native-firebase/app": "^15.6.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-navigation/stack": "^6.3.2",
    "@shopify/flash-list": "^1.3.1",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "husky": "^8.0.1",
    "pretty-quick": "^3.1.3",
    "react": "18.1.0",
    "react-native": "0.70.2",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-spinkit": "^1.5.1",
    "react-native-vector-icons": "^9.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.70.0",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "styled-components": "^5",
    "react-devtools-core": "4.24.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
